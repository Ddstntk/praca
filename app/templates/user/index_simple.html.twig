{% extends 'base.html.twig' %}
{% block body %}
<div class="row">
    {% for row in users %}
        <div class="card" style="width: 10rem; margin:15px;">
            {#<img class="card-img-top" src="data:image/png;base64,"#}
            {#alt="Profile image">#}
            <img height="200px" style="" class="card-img-top" src="{{  asset(download_photos_directory ~ '/' ~ row. photo) }}" alt="{{ row.photo }}">
            <div class="card-body">
                <h5 class="card-title">{{ row.name }} {{ row.surname }}</h5>
                <p class="card-text">Id: {{ row.PK_idUsers }}</p>
                <p class="card-text">{{ row.email }}</p>
                <p class="card-text">{{ row.birthDate }}</p>
                <p class="card-text">{{ row.birthDate }}</p>


                <a class="btn btn-info w-100 mb-2 mt-2" href="{{ url('admin_user_edit', {id: row.PK_idUsers }) }}" title="{{ 'user.edit.admin'|trans }}">
                    {{ 'user.edit.admin'|trans }}
                </a>


            </div>
        </div>
    {% endfor %}
</div>
{% endblock %}