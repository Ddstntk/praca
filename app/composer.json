{
    "name": "fabpot/silex-skeleton",
    "description": "A pre-configured skeleton for the Silex microframework",
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~5.5",
        "silex/silex": "~2.0",
        "silex/web-profiler": "~2.0",
        "symfony/asset": "~2.8",
        "symfony/browser-kit": "~2.8",
        "symfony/class-loader": "~2.8",
        "symfony/console": "~2.8",
        "symfony/css-selector": "~2.8",
        "symfony/debug": "~2.8",
        "symfony/form": "~2.8",
        "symfony/monolog-bridge": "~2.8",
        "symfony/process": "~2.8",
        "symfony/security": "2.8",
        "symfony/translation": "2.4",
        "symfony/twig-bridge": "2.8",
        "symfony/validator": "~2.8",
        "twbs/bootstrap": "4.1",
        "symfony/intl": "2.7",
        "doctrine/dbal": "2.4.*",
        "symfony/routing": "2.8",
        "symfony/config": "2.8",
        "symfony/twig-bundle": "2.8",
        "symfony/http-kernel": "2.8",
        "symfony/phpunit-bridge": "2.8",
        "symfony/web-profiler-bundle": "2.8",
        "symfony/stopwatch": "2.8",
        "symfony/security-acl": "2.8",
        "symfony/property-access": "2.8",
        "twig/twig": "~1.0",
        "symfony/http-foundation": "2.8",
        "ext-json": "^1.2",
        "pimple/pimple": "3.0.2",
        "cnam/security-jwt-service-provider": "2.*",
        "ofat/silex-jwt": "^1.0"
    },
    "autoload": {
        "psr-0": { "": "src/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts": {
        "run": [
            "echo 'Started web server on http://localhost:8888'",
            "php -S localhost:8888 -t web"
        ]
    },
    "require-dev": {
        "sorien/silex-dbal-profiler": "^2.0"
    }
}
